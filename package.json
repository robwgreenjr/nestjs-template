{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --silent --passWithNoTests",
    "test:e2e": "jest --config test/jest-e2e.json --silent --passWithNoTests",
    "test:int": "jest --config test/jest-int.json --silent --passWithNoTests",
    "test:all": "npm run test && npm run test:int && npm run test:e2e",
    "cli": "node dist/src/cli",
    "migrate": "npx mikro-orm migration:up",
    "migrate:create": "npx mikro-orm migration:create --blank",
    "migrate:down": "npx mikro-orm migration:down"
  },
  "dependencies": {
    "@automapper/classes": "8.7.7",
    "@automapper/core": "8.7.7",
    "@automapper/mikro": "8.7.7",
    "@automapper/nestjs": "8.7.7",
    "@mikro-orm/core": "5.5.3",
    "@mikro-orm/migrations": "5.5.3",
    "@mikro-orm/nestjs": "5.1.2",
    "@mikro-orm/postgresql": "5.5.3",
    "@mikro-orm/seeder": "5.5.3",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/event-emitter": "1.3.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/platform-fastify": "9.2.1",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/throttler": "3.1.0",
    "aws-sdk": "2.1267.0",
    "bcrypt": "5.1.0",
    "module-alias": "2.2.2",
    "nest-commander": "3.3.0",
    "reflect-metadata": "0.1.13",
    "require-context": "1.1.0",
    "rimraf": "3.0.2",
    "rxjs": "7.6.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.5.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.7",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.2.2",
    "nest-commander-testing": "^3.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.2",
    "testcontainers": "^9.0.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
